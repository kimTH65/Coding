N = int(input())
N_list = dict();
A = list(map(int,input().split()))
W = list(map(int,input().split()))
answer = 0


for i in range(N):
    if A[i] in N_list :
        N_list[A[i]].append(W[i])
    else :
        N_list[A[i]] = [W[i]]

print(N_list)
for k, v in N_list.items():
    while len(v) >1 :
   

len(v)
